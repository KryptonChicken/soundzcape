[MASTER]
rcfile=.pylintrc
init-hook=
ignore=CVS,.cache,__pycache__,music
ignore-patterns=
persistent=yes
load-plugins=
jobs=4
unsafe-load-any-extension=no
extension-pkg-whitelist=
optimize-ast=no

[MESSAGES CONTROL]
confidence=
enable=
disable=useless-suppression,using-cmp-argument,parameter-unpacking,basestring-builtin,long-suffix,suppressed-message,metaclass-assignment,unicode-builtin,old-ne-operator,input-builtin,standarderror-builtin,coerce-builtin,no-absolute-import,import-star-module-level,buffer-builtin,filter-builtin-not-iterating,cmp-builtin,old-raise-syntax,unichr-builtin,reduce-builtin,round-builtin,backtick,print-statement,next-method-called,old-octal-literal,dict-iter-method,coerce-method,getslice-method,range-builtin-not-iterating,file-builtin,old-division,xrange-builtin,dict-view-method,execfile-builtin,cmp-method,long-builtin,unpacking-in-except,indexing-exception,raw_input-builtin,apply-builtin,delslice-method,reload-builtin,intern-builtin,hex-method,nonzero-method,raising-string,zip-builtin-not-iterating,oct-method,setslice-method,map-builtin-not-iterating

[REPORTS]
output-format=colorized
files-output=no
reports=yes
evaluation=10.0 - ((float(5 * error + warning + refactor + convention) / statement) * 10)
msg-template=

[LOGGING]
logging-modules=logging

[MISCELLANEOUS]
notes=FIXME,TODO,FOLLOWUP

[SPELLING]
spelling-dict=
spelling-ignore-words=Soundzcape
spelling-private-dict-file=
spelling-store-unknown-words=no

[SIMILARITIES]
min-similarity-lines=4
ignore-comments=yes
ignore-docstrings=yes
ignore-imports=no

[FORMAT]
max-line-length=80
ignore-long-lines=^\s*(# )?<?https?://\S+>?$
single-line-if-stmt=no
no-space-check=trailing-comma,dict-separator
max-module-lines=1000
indent-string='    '
indent-after-paren=4
expected-line-ending-format=LF

[VARIABLES]
init-import=no
dummy-variables-rgx=(_+[a-zA-Z0-9]*?$)|dummy
additional-builtins=
callbacks=cb_,_cb
redefining-builtins-modules=six.moves,future.builtins

[BASIC]
good-names=i,j,k,e,ex,Run,_,__
bad-names=foo,bar,baz
name-group=
include-naming-hint=yes
property-classes=abc.abstractproperty
variable-rgx=[a-z_][a-z0-9_]{1,30}$
variable-name-hint=[a-z_][a-z0-9_]{1,30}$
method-rgx=[a-z_][a-z0-9_]{1,30}$
method-name-hint=[a-z_][a-z0-9_]{1,30}$
inlinevar-rgx=[A-Za-z_][A-Za-z0-9_]*$
inlinevar-name-hint=[A-Za-z_][A-Za-z0-9_]*$
attr-rgx=[a-z_][a-z0-9_]{1,30}$
attr-name-hint=[a-z_][a-z0-9_]{1,30}$
const-rgx=(([A-Z_][A-Z0-9_]*)|(__.*__))$
const-name-hint=(([A-Z_][A-Z0-9_]*)|(__.*__))$
function-rgx=[a-z_][a-z0-9_]{1,30}$
function-name-hint=[a-z_][a-z0-9_]{1,30}$
class-attribute-rgx=([A-Za-z_][A-Za-z0-9_]{1,30}|(__.*__))$
class-attribute-name-hint=([A-Za-z_][A-Za-z0-9_]{1,30}|(__.*__))$
module-rgx=(([a-z_][a-z0-9_]*)|([A-Z][a-zA-Z0-9]+))$
module-name-hint=(([a-z_][a-z0-9_]*)|([A-Z][a-zA-Z0-9]+))$
class-rgx=[A-Z_][a-zA-Z0-9]+$
class-name-hint=[A-Z_][a-zA-Z0-9]+$
argument-rgx=[a-z_][a-z0-9_]{1,30}$
argument-name-hint=[a-z_][a-z0-9_]{1,30}$
no-docstring-rgx=^(_|test_)
docstring-min-length=-1

[ELIF]
max-nested-blocks=5

[TYPECHECK]
ignore-mixin-members=yes
ignored-modules=
ignored-classes=optparse.Values,thread._local,_thread._local
generated-members=
contextmanager-decorators=contextlib.contextmanager

[IMPORTS]
deprecated-modules=optparse
import-graph=
ext-import-graph=
int-import-graph=
known-standard-library=
known-third-party=enchant
analyse-fallback-blocks=no

[DESIGN]
max-args=6
ignored-argument-names=_.*
max-locals=15
max-returns=6
max-branches=12
max-statements=50
max-parents=7
max-attributes=7
min-public-methods=2
max-public-methods=20
max-bool-expr=5

[CLASSES]
defining-attr-methods=__init__,__new__,setUp
valid-classmethod-first-arg=cls
valid-metaclass-classmethod-first-arg=mcs
exclude-protected=_asdict,_fields,_replace,_source,_make


[EXCEPTIONS]
overgeneral-exceptions=
